{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "ES6",
    "module": "CommonJS",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames":true,
    "skipLibCheck": true,
    "esModuleInterop": true,   
  },
  "include": ["src/**/*.ts"],
}